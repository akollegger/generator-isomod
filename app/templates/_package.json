{
  "name": "<%= _.slugify(moduleName) %>",
  "version": "<%= moduleVersion %>",
  "main": "dist/<%= _.slugify(moduleName) %>.js",
  "bin": {
    "<%= _.slugify(moduleName) %>": "./bin/<%= _.slugify(moduleName) %>"
  },
  "scripts": {
    "test": "grunt"
  },
  "description": "An isomorphic javascript module.",
  "homepage": "https://github.com/<%= githubUser %>/<%= _.slugify(moduleName) %>",
  "bugs": "https://github.com/<%= githubUser %>/<%= _.slugify(moduleName) %>/issues",
  "author": {
    "name": "<%= realname %>",
    "email": "<%= email %>"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= githubUser %>/<%= _.slugify(moduleName) %>"
  },
  "licenses": [
    {
      "type": "<% licenseType %>"
    }
  ],
  "dependencies": {
    "commander": "~2.1.0",
    "autocmdr": "0.0.6"
  },
  "devDependencies": {
    "grunt": "~0.4.2",
    "load-grunt-tasks": "~0.2.0",
    "grunt-contrib-jshint": "~0.6.0",
    "grunt-contrib-nodeunit": "~0.2.0",
    "grunt-contrib-watch": "~0.5.0",
    "grunt-contrib-coffee": "~0.7.0",
    "grunt-mocha-test": "~0.7.0",
    "time-grunt": "~0.1.1",
    "jshint-stylish": "~0.1.3",
    "grunt-coffeelint": "0.0.8",
    "grunt-contrib-requirejs": "~0.4.1",
    "grunt-contrib-clean": "~0.5.0",
    "coffee-script": "~1.6.3",
    "grunt-sync-pkg": "~0.1.2",
    "should": "~2.1.1",
    "codo": "~2.0.3",
    "bower": "~1.2.8", 
    "grunt-bower-requirejs": "~0.8.4"
  }
}
